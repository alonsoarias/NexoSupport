{
  "name": "iser/nexosupport",
  "description": "NexoSupport - Sistema de GestiÃ³n con arquitectura Frankenstyle",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.1",
    "mustache/mustache": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "core\\": "lib/classes/",
      "auth_manual\\": "auth/manual/classes/",
      "theme_core\\": "theme/core/classes/",
      "theme_boost\\": "theme/boost/classes/",
      "tool_mfa\\": "admin/tool/mfa/classes/",
      "factor_email\\": "admin/tool/mfa/factor/email/classes/",
      "report_log\\": "report/log/classes/",
      "report_loglive\\": "report/loglive/classes/",
      "report_security\\": "report/security/classes/",
      "report_performance\\": "report/performance/classes/"
    },
    "files": [
      "lib/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
